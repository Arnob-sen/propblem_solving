#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
int main()
{
    int t;
    cin >> t;
    while (t--)
    {
        int d, k;
        cin >> d >> k;
        ll ans = 0;
        for (int i = 0; i < d; i++)
        {
            int x;
            cin >> x;
            if (x > k)
                ans += (x - k);
        }
        cout << ans << endl;
    }
}
